    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/PoTTr.Backend.Google/bin/Debug/netcoreapp3.1/PoTTr.Backend.Google.exe",
            "args": ["--audio-file=Episodes/2019-10-30--t01-17-20am--collin-jurenka.mp3","google","--json-credential-path", "Episodes/2019-10-30--t01-17-20am--collin-jurenka.mp3"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}